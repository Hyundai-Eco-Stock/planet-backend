<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.phoenix.planet.mapper.DepartmentStoreMapper">
    
    <select id="selectAll"
        resultType="org.phoenix.planet.dto.department_store.response.DepartmentStoreListResponse">
        SELECT
        department_store_id AS departmentStoreId,
        name,
        lat,
        lng
        FROM department_store
        ORDER BY department_store_id
    </select>

</mapper>